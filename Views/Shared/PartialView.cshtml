@{
    var listBookMaster = ViewData["List"] as List<Library.Models.BookMaster>;
 }

<table>
    <tr>
        <th>BookName</th>
        <th>PublishDate</th>
        <th>Available</th>
        <th>Image</th>
        <th></th>
        <th></th>
    </tr>
    @foreach(var item in listBookMaster)
    { 
     <tr>
         <td>@item.BookName</td>
         <td>@item.PublishDate</td>
         <td>@item.Available</td>
         <td><img src="@item.Image" id="img" width="120" height="150" /></td>
         <td><a href="/Home/Index?id=@item.BookId">Edit</a></td>
         <td><a href="/Home/IndexDelete?id=@item.BookId">Delete</a></td>
     </tr>
    }
</table>
